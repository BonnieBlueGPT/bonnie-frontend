{
  "cleanUrls": true,
  "trailingSlash": false,
  "buildCommand": "echo 'No build needed - serving static HTML'",
  "devCommand": null,
  "installCommand": "echo 'No install needed'",
  "outputDirectory": ".",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}